public class Solution {
    /**
     * @param n: an integer
     * @return: an ineger f(n)
     */
    public int fibonacci(int n) {
        if(n==2 || n==3){
            return 1;
        }
        else if(n<=1){
            return 0;
        }
        else{
            return fibonacci(n-1)+fibonacci(n-2);
        }
    }
}

